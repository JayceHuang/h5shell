<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="txwp_picker_black">#000000</color>
    <color name="txwp_picker_transparent">#00000000</color>
    <color name="txwp_status_bar">#303135</color>
    <color name="txwp_theme_body">#cc22292c</color>
    <color name="txwp_top_bar">#9f556061</color>
    <declare-styleable name="CropImageView"><attr format="color" name="cropMaskColor"/><attr format="color" name="cropBorderColor"/><attr format="dimension" name="cropBorderWidth"/><attr format="dimension" name="cropFocusWidth"/><attr format="dimension" name="cropFocusHeight"/><attr format="enum" name="cropStyle"><enum name="rectangle" value="0"/><enum name="circle" value="1"/></attr></declare-styleable>
    <string name="txwp_all_images">全部图片</string>
    <string name="txwp_complete">完成</string>
    <string name="txwp_folder_image_count">共%1$d张</string>
    <string name="txwp_origin">原图</string>
    <string name="txwp_origin_size">原图(%1$s)</string>
    <string name="txwp_photo_crop">图片裁剪</string>
    <string name="txwp_preview_count">预览(%1$s)</string>
    <string name="txwp_preview_image_count">%1$s/%2$s</string>
    <string name="txwp_select_complete">完成(%1$s/%2$s)</string>
    <string name="txwp_select_limit">最多选择%1$s张图片</string>
    <style name="Txwp.ImagePickerTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:fitsSystemWindows">true</item>
    </style>
    <style name="Txwp.ImagePickerThemeFullScreen" parent="Txwp.ImagePickerTheme">
        <item name="android:fitsSystemWindows">false</item>
    </style>
    <style name="Txwp.SuperCheckboxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/txwp_selector_item_checked</item>
    </style>
    <style name="popupwindow_anim_style">
        <!-- 指定显示的动画xml -->
        <item name="android:windowEnterAnimation">@anim/txwp_show_from_bottom</item>
        <!-- 指定消失的动画xml -->
        <item name="android:windowExitAnimation">@anim/txwp_hide_to_bottom</item>
    </style>
</resources>